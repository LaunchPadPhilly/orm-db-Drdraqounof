    "react-dom": "^18.0.0",
    "sass": "^1.94.2",
    "three": "^0.181.2"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.17",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^9.2.1",
    "eslint": "^8.56.0",
    "eslint-config-next": "^15.0.3",
    "jsdom": "^23.0.1",
    "postcss": "^8.4.32",
